/* based on openwrt15/target/linux/ramips/dts/mt7628an.dtsi */

/dts-v1/;

/ {
	#address-cells = <1>;
	#size-cells = <1>;
	compatible = "ralink,mtk7628an-soc";

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			compatible = "mips,mips24KEc";
			clocks = <&cpupll>;
			reg = <0>;
		};
	};

	cpupll: cpupll {
		compatible = "fixed-clock";
		#clock-cells = <0>;
	};

	bbppll12: bbppll12 {
		compatible = "fixed-clock";
		#clock-cells = <0>;
	};

	palmbus@10000000 {
		compatible = "simple-bus";
                ranges = <0x0 0x10000000 0x1FFFFF>;

		#address-cells = <1>;
		#size-cells = <1>;

		uartlite: uartlite@c00 {
			compatible = "ns16550a";
			reg = <0xc00 0x100>;

			reg-shift = <2>;
			reg-io-width = <4>;
			no-loopback-test;

			status = "disabled";
		};

		uart1: uart1@d00 {
			compatible = "ns16550a";
			reg = <0xd00 0x100>;

			reg-shift = <2>;
			reg-io-width = <4>;
			no-loopback-test;

			status = "disabled";
		};

		uart2: uart2@e00 {
			compatible = "ns16550a";
			reg = <0xe00 0x100>;

			reg-shift = <2>;
			reg-io-width = <4>;
			no-loopback-test;

			status = "disabled";
		};
	};
};
